{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  description: "Automated dependency updates for multiplatform-packer-vsphere-actions",
  extends: [
    "config:recommended",
    "docker:enableMajor",
    "replacements:k8s-registry-move",
    ":automergePr",
    ":automergePatch",
    ":automergeDigest",
    ":disableRateLimiting",
    ":dependencyDashboard",
    ":semanticCommits",
    ":timezone(Europe/London)",
    "github>sudo-kraken/multiplatform-packer-vsphere-actions//.github/renovate/devcontainers.json5",
    "github>sudo-kraken/multiplatform-packer-vsphere-actions//.github/renovate/labels.json5",
    "github>sudo-kraken/multiplatform-packer-vsphere-actions//.github/renovate/semanticCommits.json5",
    "github>sudo-kraken/multiplatform-packer-vsphere-actions//.github/renovate/packer.json5",
  ],
  dependencyDashboardTitle: "Renovate Dashboard ðŸ¤–",
  suppressNotifications: ["prEditedNotification", "prIgnoreNotification"],
  rebaseWhen: "conflicted",
  schedule: ["before 9pm on friday"],
  prConcurrentLimit: 5,
  labels: ["dependencies"],
  minimumReleaseAge: "7 days",

  packageRules: [
    {
      matchManagers: ["github-actions"],
      minimumReleaseAge: null,
    },
  ],
}
